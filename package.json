{
  "name": "Car-rent",
  "version": "1.0.0",
  "description": "Api to link cars to rent to users",
  "scripts": {
    "start": "node dist/index.js",
    "start:dev": "pnpm run database:up & tsx watch src/index.ts",
    "build": "tsup build src",
    "app": "docker compose up -d",
    "app:down": "docker compose down --remove-orphans",
    "database:up": "tsx scripts/startDatabase.ts",
    "database:down": "docker volume rm car-rent_db",
    "test": "vitest",
    "lint": "eslint \"src/**/*.ts\" --fix"
  },
  "keywords": [
    "car-rent",
    "api",
    "nodejs",
    "express"
  ],
  "author": "devamaral2",
  "license": "ISC",
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/node": "^20.11.0",
    "eslint": "^8.56.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "prettier": "^3.2.2",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "pg-promise": "^11.5.4",
    "zod": "^3.22.4"
  }
}